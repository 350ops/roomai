{
  "expo": {
    "newArchEnabled": true,
    "name": "reForma",
    "slug": "reforma",
    "version": "1.0.0",
    "runtimeVersion": "1.0.0",
    "scheme": "reforma",
    "platforms": [
      "ios"
    ],
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash-icon.png",
          "imageWidth": 200
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "This app needs access to your photos to select room images for AI renovation.",
          "cameraPermission": "This app needs access to your camera to take photos of rooms for AI renovation."
        }
      ],
      "expo-roomplan",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "17.0"
          }
        }
      ],
      [
        "react-native-file-viewer-turbo",
        {
          "ios": {
            "mimeTypes": [
              "*/*"
            ]
          }
        }
      ]
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mmdev13.reforma",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "This app needs access to your photos to select room images for AI renovation.",
        "NSCameraUsageDescription": "This app needs access to your camera to take photos of rooms for AI renovation."
      },
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": [
              "CA92.1"
            ]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryFileTimestamp",
            "NSPrivacyAccessedAPITypeReasons": [
              "0A2A.1",
              "3B52.1",
              "C617.1"
            ]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryDiskSpace",
            "NSPrivacyAccessedAPITypeReasons": [
              "E174.1",
              "85F4.1"
            ]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategorySystemBootTime",
            "NSPrivacyAccessedAPITypeReasons": [
              "35F9.1"
            ]
          }
        ],
        "NSPrivacyCollectedDataTypes": [],
        "NSPrivacyTracking": false
      },
      "appleTeamId": "ANAZ4TXPX5"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "81ab9df2-2916-468f-bc53-df92626bf312"
      }
    },
    "owner": "mmdev13"
  }
}
